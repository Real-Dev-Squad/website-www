      "assets/icons/github-logo.png",
      "assets/icons/github-white.png",
      "assets/icons/instagram-logo.png",
      "assets/icons/instagram-white.png",
      "assets/icons/linkedin-logo.png",
      "assets/icons/linkedin-white.png",

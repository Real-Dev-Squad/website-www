    "ember-welcome-page": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "loader.js": "^4.7.0",
    "local-ssl-proxy": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "toastr": "^2.1.4",
    "webpack": "^5.84.1"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "node": "18.12.0",
    "yarn": "1.22.19"
  },
  "pre-commit": {
    "run": "lint"
  },
  "fastboot": {
    "schemaVersion": 5,
    "htmlEntrypoint": "index.html",
    "moduleWhitelist": [
      "node-fetch",
      "abortcontroller-polyfill",
      "abortcontroller-polyfill/dist/cjs-ponyfill"
    ],
    "hostWhitelist": [
      "/^localhost:\\d+$/",
      "dev.realdevsquad.com"
    ]
  },
  "keywords": [
    "ember-addon",
    "ember-addon"
  ],

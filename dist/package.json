{
  "name": "website-www",
  "version": "0.0.0",
  "private": true,
  "description": "Landing Page of the Real Dev Squad",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "dev": "npm-run-all --aggregate-output --continue-on-error --parallel dev:*",
    "dev:ember": "ember server -p 4200",
    "dev:reverse-ssl": "local-ssl-proxy --source 443 --target 4200",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test"
  },
  "dependencies": {
    "@100mslive/hms-video-store": "^0.10.6",
    "dotenv": "^16.0.2",
    "fastboot-app-server": "^3.3.2",
    "abortcontroller-polyfill": "^1.7.3",
    "node-fetch": "^2.6.1"
  },
